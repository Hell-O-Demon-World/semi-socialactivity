<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>main.jsp</welcome-file>
        <welcome-file>main.html</welcome-file>
        <welcome-file>main.htm</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.golfzone.social.controller.auth.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>com.golfzone.social.controller.auth.SignUpController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>club</servlet-name>
        <servlet-class>com.golfzone.social.controller.club.ClubController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>club</servlet-name>
        <url-pattern>/club</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>createclub</servlet-name>
        <servlet-class>com.golfzone.social.controller.club.CreateClubController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>createclub</servlet-name>
        <url-pattern>/club/createclub</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>clubinfo</servlet-name>
        <servlet-class>com.golfzone.social.controller.club.ClubInfoController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>clubinfo</servlet-name>
        <url-pattern>/clubinfo</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>joinclub</servlet-name>
        <servlet-class>com.golfzone.social.controller.club.JoinClubController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>joinclub</servlet-name>
        <url-pattern>/club/joinclub</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>joinactivity</servlet-name>
        <servlet-class>com.golfzone.social.controller.activity.JoinActivityController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>joinactivity</servlet-name>
        <url-pattern>/joinactivity</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>mypage</servlet-name>
        <servlet-class>com.golfzone.social.controller.mypage.MyPageController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mypage</servlet-name>
        <url-pattern>/mypage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>quitactivity</servlet-name>
        <servlet-class>com.golfzone.social.controller.activity.QuitActivityController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>quitactivity</servlet-name>
        <url-pattern>/quitactivity</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>searchclub</servlet-name>
        <servlet-class>com.golfzone.social.controller.search.SearchClubController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>searchclub</servlet-name>
        <url-pattern>/searchclub</url-pattern>
    </servlet-mapping>
</web-app>